<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back"
    tools:context="com.example.laptop.satisficationtest.TestActivity">

    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:orientation="vertical"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">


        <ImageButton
            android:id="@+id/tressatisfai"
            android:layout_width="match_parent"
            android:layout_height="122dp"

            app:srcCompat="@drawable/tressatisfait" />

        <ImageButton
            android:id="@+id/satisfait"
            android:layout_width="match_parent"
            android:layout_height="120dp"
            app:srcCompat="@drawable/satisfait"

            tools:layout_editor_absoluteX="51dp"
            tools:layout_editor_absoluteY="158dp" />

        <ImageButton
            android:id="@+id/insatisfait"
            android:layout_width="match_parent"

            android:layout_height="128dp"
            app:srcCompat="@drawable/insatisfait" />
        <ImageButton
            android:id="@+id/tresinstisfait"
            android:layout_width="match_parent"

            android:layout_height="wrap_content"
            app:srcCompat="@drawable/tresinsatisfait"
            tools:layout_editor_absoluteX="51dp"
            tools:layout_editor_absoluteY="158dp" />

    </LinearLayout>

</android.support.constraint.ConstraintLayout>
